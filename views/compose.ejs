<%- include("partials/header") -%> 
<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
        <label for="composeTitle">Title</label>
        <input class="form-control form-control-lg" id="composeTitle" type="text" name="composeTitle" placeholder="Add title">
    </div>
    <div class="form-group">
        <label for="composeText">Post</label>
        <textarea class="form-control" id="composeText" name="composeText" rows="5"></textarea>
    </div>

    
    <button class="btn btn-primary" type="submit">Publish</button>
</form>

<%- include("partials/footer") -%>